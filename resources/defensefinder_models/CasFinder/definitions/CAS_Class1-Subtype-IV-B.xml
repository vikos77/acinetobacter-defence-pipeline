<model inter_gene_max_space="3" min_mandatory_genes_required="1" min_genes_required="2" max_nb_genes="4" vers="2.0">

### csf4gr11 - cas11 ###
<gene name="csf4gr11_IV-B_2" presence="mandatory">
    <exchangeables>
        <gene name="csf4gr11_IV-B_4"/>
	</exchangeables>
</gene>


### csf2gr7 - cas7 ###
<gene name="csf2gr7_IV-B_1" presence="mandatory"></gene>
<gene name="csf2gr7_IV_1" presence="accessory"></gene>

### csf3gr5 - cas5 ###
<gene name="csf3gr5_IV-B_1" presence="mandatory"></gene>
 
### csf1gr8 - cas8-like ###
<gene name="csf1gr8_IV-B_1" presence="accessory">
    <exchangeables>
        <gene name="csf1gr8_IV-A_2"/>
        <gene name="csf1gr8_IV-A_3"/>
        <gene name="csf1gr8_IV-A_5"/>
        <gene name="csf1gr8_IV-A_6"/>
        <gene name="csf1gr8_IV_1"/>
	</exchangeables>
</gene>

### cysH-like ###
<gene name="cysH_IV-B_1" presence="neutral"> </gene>

### csf5 - cas6 ###
<gene name="csf5gr6_IV-A_1" presence="accessory">
       <exchangeables>
          <gene name="cas6e_I_II_III_IV_V_VI_2"/>
          <gene name="cas6e_I_II_III_IV_V_VI_3"/>
          <gene name="cas6_I_II_III_IV_V_VI_22"/>
          <gene name="cas6_I_II_III_IV_V_VI_7"/>
          <gene name="cas6_I_II_III_IV_V_VI_10"/>
          <gene name="cas6_I_II_III_IV_V_VI_11"/>
          <gene name="cas6_I_II_III_IV_V_VI_12"/>
          <gene name="cas6_I_II_III_IV_V_VI_13"/>
          <gene name="cas6_I_II_III_IV_V_VI_14"/>
          <gene name="cas6_I_II_III_IV_V_VI_15"/>
          <gene name="cas6_I_II_III_IV_V_VI_17"/>
          <gene name="cas6_I_II_III_IV_V_VI_20"/>   
      </exchangeables>
</gene>

#########################
# neutral genes
#########################
<gene name="DEDDh_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>
<gene name="WYL_I_II_III_IV_V_VI_4" presence="neutral" inter_gene_max_space="1"> </gene>


</model>