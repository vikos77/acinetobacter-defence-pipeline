<model inter_gene_max_space="3" min_mandatory_genes_required="1" min_genes_required="2" max_nb_genes="20" vers="2.0">

### csf1gr8 - cas8-like ###
<gene name="csf1gr8_IV-A_3" presence="mandatory">
