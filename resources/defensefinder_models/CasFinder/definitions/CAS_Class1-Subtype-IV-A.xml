<model inter_gene_max_space="3" min_mandatory_genes_required="1" min_genes_required="2" max_nb_genes="5" vers="2.0">

### csf1gr8 - cas8-like ###
<gene name="csf1gr8_IV-A_3" presence="mandatory">
    <exchangeables>
        <gene name="csf1gr8_IV-A_2"/>
        <gene name="csf1gr8_IV-A_5"/>
        <gene name="csf1gr8_IV-A_6"/>
	</exchangeables>
</gene>

<gene name="csf1gr8_IV_1" presence="accessory"> </gene>

### csf2gr7 - cas7 ###
<gene name="csf2gr7_IV-A_1" presence="mandatory"></gene>
<gene name="csf2gr7_IV_1" presence="accessory"></gene>

### csf3gr5 - cas5 ###
<gene name="csf3gr5_IV-A_1" presence="mandatory"> #IVA
    <exchangeables>
        <gene name="csf3gr5_IV-A_2"/> #IVA
    </exchangeables>
</gene>


### csf4 - dinG ###
<gene name="csf4_IV-A_1" presence="accessory"> #IVA
    <exchangeables>
        <gene name="csf4_IV-A1_1"/> #IVA
        <gene name="csf4_IV-A1_2"/> #IVA
    </exchangeables>
</gene>


### csf5 - cas6 ###
<gene name="csf5gr6_IV-A_1" presence="mandatory"> </gene>

<gene name="cas6e_I_II_III_IV_V_VI_3" presence="accessory">
    <exchangeables>
          <gene name="cas6f_I_II_III_IV_V_VI_3"/>
          <gene name="cas6_I_II_III_IV_V_VI_15"/>
          <gene name="cas6e_I_II_III_IV_V_VI_2"/>
       	  <gene name="cas6_I_II_III_IV_V_VI_22"/>
          <gene name="cas6_I_II_III_IV_V_VI_7"/>
          <gene name="cas6_I_II_III_IV_V_VI_10"/>
          <gene name="cas6_I_II_III_IV_V_VI_11"/>
          <gene name="cas6_I_II_III_IV_V_VI_12"/>
          <gene name="cas6_I_II_III_IV_V_VI_13"/>
          <gene name="cas6_I_II_III_IV_V_VI_14"/>
          <gene name="cas6_I_II_III_IV_V_VI_17"/>
          <gene name="cas6_I_II_III_IV_V_VI_20"/>
    </exchangeables>
</gene>


#########################
# forbidden genes
#########################

### csf4gr11 - cas11 ###
<gene name="csf4gr11_IV-B_2" presence="forbidden">
    <exchangeables>
        <gene name="csf4gr11_IV-B_4"/>
    </exchangeables>
</gene>

### LS ###
<gene name="LSIVC_IV-C_1" presence="forbidden"> </gene>
### SS ###
<gene name="SSIVC_IV-C_1" presence="forbidden"> </gene>

#########################
# neutral genes
#########################


<gene name="cas1_I-F_1" presence="accessory" inter_gene_max_space="1"> </gene>
<gene name="cas1_I-E_1" presence="accessory" inter_gene_max_space="1"> </gene>
<gene name="DEDDh_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>
<gene name="RT_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>
<gene name="csm3gr7_III_IV" presence="accessory" inter_gene_max_space="1"> </gene>
<gene name="PD_DExK_I_II_III_IV_V_VI_1" presence="accessory" inter_gene_max_space="1"> </gene>




</model>