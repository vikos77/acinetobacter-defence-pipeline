<model inter_gene_max_space="3" min_mandatory_genes_required="1" min_genes_required="2" max_nb_genes="8" vers="2.0">

### cas1 ###
<gene name="cas1_I-E_1" presence="mandatory"></gene>
 
### cas2 ###
<gene name="cas2_I-E_1" presence="accessory">
      <exchangeables>
         <gene name="cas2_I-E_2"/> #I-E
         <gene name="cas2_I_II_III_IV_V_VI_4"/>  #I-E
         <gene name="cas2_I_II_III_IV_V_VI_5"/>
         <gene name="cas2_I_II_III_IV_V_VI_9"/>
         <gene name="cas2_I_II_III_IV_V_VI_1"/>
         <gene name="cas2_I_II_III_IV_V_VI_13"/>
      </exchangeables>
</gene>

### cas3 ###
<gene name="cas3_I_5" presence="accessory" multi_model="1">
      <exchangeables>
         <gene name="cas3_I_2"/>
      </exchangeables>
</gene>

<gene name="cas3HD_I-E_4" presence="accessory">
      <exchangeables>
         <gene name="cas3HD_I_1"/>
         <gene name="cas3HD_I_3"/>
      </exchangeables>
</gene>

### cas5 ###
<gene name="cas5_I-E_3" presence="mandatory">
      <exchangeables>
         <gene name="cas5_I-E_13"/>
         <gene name="cas5_I-E_2"/>
      </exchangeables>
</gene>

<gene name="cas5_I_9" presence="accessory"></gene>

### cas7 ###
<gene name="cas7_I-E_16" presence="mandatory">
     <exchangeables>
        <gene name="cas7_I-E_2"/>
     </exchangeables>
</gene>

### cas8 ###
<gene name="cas8e_I-E_1" presence="mandatory">
     <exchangeables>
        <gene name="cas8e_I-E_2"/>
        <gene name="cas8e_I-E_3"/>
        <gene name="cas8e_I-E_4"/>
        <gene name="cas8e_I-E_5"/>
     </exchangeables>
</gene>

### cse2gr11 - cas11 ###
<gene name="cse2gr11_I-E_2" presence="mandatory">
     <exchangeables>
        <gene name="cse2gr11_I-E_1"/>
        <gene name="cse2gr11_I-E_3"/>
        <gene name="cse2gr11_I-E_4"/>
        <gene name="cse2gr11_I-E_5"/>
        <gene name="cse2gr11_I-E_6"/>
        <gene name="cse2gr11_I-E_7"/>
        <gene name="cse2gr11_I-E_8"/>
     </exchangeables>
</gene>

### cas6 ###
<gene name="cas6e_I_II_III_IV_V_VI_1" presence="mandatory">
    <exchangeables>
          <gene name="cas6e_I_II_III_IV_V_VI_2"/>
          <gene name="cas6e_I_II_III_IV_V_VI_3"/>
    </exchangeables>
</gene>

### cas4 ###
<gene name="cas4_I_II_III_IV_V_VI_6" presence="accessory"> 
    <exchangeables>
          <gene name="cas4_I_II_III_IV_V_VI_1"/>
    </exchangeables>
</gene>

#########################
# neutral genes
#########################

# RT
<gene name="RT_I-E_1" presence="neutral" inter_gene_max_space="1">
    <exchangeables>
        <gene name="RT_I_II_III_IV_V_VI_1"/>
    </exchangeables>
</gene>

# WYL
<gene name="WYL_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1">
    <exchangeables>
        <gene name="WYL_I_II_III_IV_V_VI_4"/>
    </exchangeables>
</gene>

# DEDDh
<gene name="DEDDh_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>

# casR
<gene name="casR_I_1" presence="neutral" inter_gene_max_space="1"> 
    <exchangeables>
        <gene name="casR_I_2"/>
    </exchangeables>
</gene>

# PrimPol
 <gene name="PrimPol_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>


</model>
