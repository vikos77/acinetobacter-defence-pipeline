<model inter_gene_max_space="3" min_mandatory_genes_required="1" min_genes_required="2" max_nb_genes="6" vers="2.0">

# cas1
<gene name="cas1_I-F_1" presence="mandatory"></gene>

# cas3
<gene name="cas3f_I-F_1" presence="mandatory"> </gene>
      
<gene name="cas3_I_5" presence="accessory"> 
      <exchangeables>
         <gene name="cas3_I_2"/>
         <gene name="cas3HD_I_1"/>
         <gene name="cas3HD_I_3"/>
      </exchangeables>
</gene>

# cas5
<gene name="cas5f_I-F_4" presence="mandatory">
      <exchangeables>
        <gene name="cas5f_I-F_1"/>
        <gene name="cas5f_I-F_2"/>
        <gene name="cas5f_I-F_3"/>
        <gene name="cas5_I-F_16"/>
      </exchangeables>
</gene>

<gene name="cas5_I_9" presence="accessory"></gene>

# cas7
<gene name="cas7f_I-F_3" presence="mandatory">
      <exchangeables>
        <gene name="cas7f_I-F_1"/>
        <gene name="cas7f_I-F_2"/>
        <gene name="cas7_I-F_11"/>
      </exchangeables>
</gene>

# cas8
<gene name="cas8f_I-F_8" presence="mandatory">
     <exchangeables>
        <gene name="cas8f_I-F_2"/>
        <gene name="cas8f_I-F_3"/>
        <gene name="cas8f_I-F_4"/>
        <gene name="cas8f_I-F_5"/>
        <gene name="cas8f_I-F_6"/>
        <gene name="cas8f_I-F_7"/>
     </exchangeables>
</gene>

# cas6
<gene name="cas6f_I_II_III_IV_V_VI_3" presence="accessory" multi_model="1">
       <exchangeables>
           <gene name="cas6f_I_II_III_IV_V_VI_2"/>
          <gene name="cas6f_I_II_III_IV_V_VI_1"/>
      </exchangeables>
</gene>


#########################
# neutral genes
#########################
# cas2
<gene name="cas2_I_II_III_IV_V_VI_1" presence="neutral">
       <exchangeables>
         <gene name="cas2_I_II_III_IV_V_VI_3"/>
         <gene name="cas2_I_II_III_IV_V_VI_5"/>
         <gene name="cas2_I_II_III_IV_V_VI_9"/>
         <gene name="cas2_I_II_III_IV_V_VI_13"/>
    </exchangeables>
</gene>

# DEDDh
<gene name="DEDDh_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>

# TniQ
<gene name="TniQ_I-F_1" presence="neutral" inter_gene_max_space="1"> </gene>

# WYL
<gene name="WYL_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1">
    <exchangeables>
        <gene name="WYL_I_II_III_IV_V_VI_4"/>
    </exchangeables>
</gene>

# casR
<gene name="casR_I_1" presence="neutral" inter_gene_max_space="1"> 
    <exchangeables>
        <gene name="casR_I_2"/>
    </exchangeables>
</gene>

# PD_DExK
<gene name="PD_DExK_I_II_III_IV_V_VI_4" presence="neutral" inter_gene_max_space="1"> </gene>

# RT
<gene name="RT_I_II_III_IV_V_VI_1" presence="neutral" inter_gene_max_space="1"> </gene>
</model>
